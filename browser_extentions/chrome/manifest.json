{
  "manifest_version": 2,
  "name": "VK Disk Extension",
  "version": "2018.03.13",

  "description": "Our Extention for VK Disk",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "*://localhost/*"
  ],

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}